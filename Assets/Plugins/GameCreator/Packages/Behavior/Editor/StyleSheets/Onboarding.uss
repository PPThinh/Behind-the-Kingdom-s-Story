GraphOnboarding {
    width: 50%;
    height: 100%;
    margin: 0 25%;
    padding: 0;
    flex-direction: column;
    flex-wrap: nowrap;
}

GraphOnboarding #GC-Graph-Onboarding-Content {
    width: 100%;
    flex-grow: 0;
    flex-shrink: 1;
}

GraphOnboarding #GC-Graph-Onboarding-Content Button {
    margin: var(--gc-common-space-small) 0;
    padding: var(--gc-common-space-small) var(--gc-common-space-smaller);
}