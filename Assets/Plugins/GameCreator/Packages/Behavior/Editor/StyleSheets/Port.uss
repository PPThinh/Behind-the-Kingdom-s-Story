#GC-Graph-Port-Root {
    width: var(--gc-graph-port-size);
    height: var(--gc-graph-port-size);
    margin: 0;
    padding: 0;
    border-radius: var(--gc-graph-port-radius);
    flex-grow: 0;
    flex-shrink: 0;
}

#GC-Graph-Port-Root Label {
    width: 100%;
    height: 100%;
    color: var(--gc-color-bg-darker);
    -unity-font-style: bold;
    -unity-text-align: middle-center;
}